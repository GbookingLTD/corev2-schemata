{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "schemas-json/controllers/crackSlots/CRACDistributedResourcesFreeByDate.response.schema.json",
    "title": "Crac.CRACDistributedResourcesFreeByDate.response",
    "type": "object",
    "definitions": {
        "ErrorCodes": {
            "type": "object",
            "required": [
                "error"
            ],
            "additionalProperties": true,
            "properties": {
                "error": {
                    "oneOf": [
                        {
                            "$ref": "../../common/partials/authErrorCodes.schema.json"
                        },
                        {
                            "type": "object",
                            "required": [
                                "code"
                            ],
                            "additionalProperties": true,
                            "properties": {
                                "code": {
                                    "description": "код ошибки",
                                    "type": "number",
                                    "enum": [
                                        -10000,
                                        -70300
                                    ],
                                    "meta:enum": {
                                        "-10000": "UNKNOWN_ERROR - Unknown error occurred",
                                        "-70300": "CRAC_PROXY_ERROR"
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        },
        "ErrorResponse": {
            "allOf": [
                {
                    "$ref": "../../common/errorResponse.schema.json"
                },
                {
                    "$ref": "#/definitions/ErrorCodes"
                }
            ]
        },
        "Free": {
            "type": "object",
            "additionalProperties": true,
            "required": [
                "Free"
            ],
            "properties": {
                "Free": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": true,
                        "required": [
                            "resource",
                            "taxonomy",
                            "date",
                            "maxFreeMinutes"
                        ],
                        "properties": {
                            "resource": {
                                "type": "string"
                            },
                            "taxonomy": {
                                "type": "string"
                            },
                            "date": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "maxFreeMinutes": {
                                "type": "number"
                            }
                        }
                    }
                }
            }
        },
        "SuccessResponse": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "id",
                "result",
                "error"
            ],
            "properties": {
                "id": {
                    "description": "значение числового типа для идентификации запроса на сервере",
                    "type": "number"
                },
                "result": {
                    "$ref": "#/definitions/Free"
                },
                "error": {
                    "type": [
                        "null",
                        "object"
                    ]
                }
            }
        }
    },
    "oneOf": [
        {
            "$ref": "#/definitions/SuccessResponse"
        },
        {
            "$ref": "#/definitions/ErrorResponse"
        }
    ]
}